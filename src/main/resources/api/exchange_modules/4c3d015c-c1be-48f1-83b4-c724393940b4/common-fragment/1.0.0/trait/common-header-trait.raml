#%RAML 1.0 Trait

headers:
  x-fms-transactionId:
    required: true
    maxLength: 36
    minLength: 36
    type: string
    example: "123456789-qwertyuio-asdfghjkl-123456"
  x-fms-clientId:
    required: true
    type: string
    example: "45678etyrfuigh5678"
  x-fms-clientSecret:
    required: true
    type: string
    example: "56789fghjvbntyui67"